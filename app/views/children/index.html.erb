<h3>Children in System</h3>
<table>
  <tr>
    <th>Name</th>
    <th>Classrooom</th>
  </tr>
  <% @children.each do |child| %>
  <tr>
    <td><%= link_to child.full_name, edit_child_path(child) %></td>
    <% if child.class_room_id %>
      <td><%= link_to child.room_name, class_room_path(child.class_room) %></td>
    <% else %>
      <td><%= child.room_name %></td>
    <% end %>
  </tr>
  <% end %>
</table>